<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.anna.firstapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".EntryActivity" />
        <activity android:name=".ColorActivity" />
        <activity android:name=".Color2Activity" />
        <activity android:name=".Color3Activity" />
        <activity android:name=".Color4Activity" />
        <activity android:name=".StarActivity" />
        <activity android:name=".ZoomActivity" />
        <activity android:name=".CameraActivity"></activity>
    </application>

</manifest>